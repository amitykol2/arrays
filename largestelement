#include<stdio.h>
int main()
{
    int arr[]={4,2,1,7,19};
    int max,min= arr[0];
    for(int i=0; i<5;i++)
    {
        if (arr[i]>max)
        {
            max=arr[i];
        }
    }
    printf("The largest element is %d\n",max);
    for(int i=0; i<5;i++)
    {
        if (arr[i]<min)
        {
            min=arr[i];
        }
    }
    printf("The smallest element is %d\n",min);
    return 0;
    }
